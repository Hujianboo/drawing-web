#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier@2.1.2/node_modules/prettier/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier@2.1.2/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier-plugin-import-sort@0.0.7_prettier@2.1.2/node_modules/prettier-plugin-import-sort/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier-plugin-import-sort@0.0.7_prettier@2.1.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier@2.1.2/node_modules/prettier/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier@2.1.2/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier-plugin-import-sort@0.0.7_prettier@2.1.2/node_modules/prettier-plugin-import-sort/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/prettier-plugin-import-sort@0.0.7_prettier@2.1.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.1.2/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.1.2/node_modules/prettier/bin-prettier.js" "$@"
fi
