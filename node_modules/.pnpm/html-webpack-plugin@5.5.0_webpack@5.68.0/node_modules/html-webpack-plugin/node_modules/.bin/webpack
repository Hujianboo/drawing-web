#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack@5.68.0/node_modules/webpack/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack@5.68.0/node_modules/webpack/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack@5.68.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-webpack-plugin@5.5.0_webpack@5.68.0/node_modules/html-webpack-plugin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-webpack-plugin@5.5.0_webpack@5.68.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack@5.68.0/node_modules/webpack/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack@5.68.0/node_modules/webpack/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack@5.68.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-webpack-plugin@5.5.0_webpack@5.68.0/node_modules/html-webpack-plugin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-webpack-plugin@5.5.0_webpack@5.68.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.68.0/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.68.0/node_modules/webpack/bin/webpack.js" "$@"
fi
