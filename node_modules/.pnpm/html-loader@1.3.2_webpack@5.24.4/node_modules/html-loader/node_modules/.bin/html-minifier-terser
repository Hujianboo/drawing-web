#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-loader@1.3.2_webpack@5.24.4/node_modules/html-loader/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-loader@1.3.2_webpack@5.24.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-loader@1.3.2_webpack@5.24.4/node_modules/html-loader/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/html-loader@1.3.2_webpack@5.24.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
fi
