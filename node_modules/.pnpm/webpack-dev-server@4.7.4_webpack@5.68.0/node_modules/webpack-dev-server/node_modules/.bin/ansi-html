#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack-dev-server@4.7.4_webpack@5.68.0/node_modules/webpack-dev-server/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack-dev-server@4.7.4_webpack@5.68.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack-dev-server@4.7.4_webpack@5.68.0/node_modules/webpack-dev-server/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/webpack-dev-server@4.7.4_webpack@5.68.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
else
  exec node  "$basedir/../../../../../ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/ansi-html" "$@"
fi
