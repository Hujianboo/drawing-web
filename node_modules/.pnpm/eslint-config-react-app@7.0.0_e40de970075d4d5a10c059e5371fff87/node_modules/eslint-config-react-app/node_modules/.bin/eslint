#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@8.9.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-react-app@7.0.0_e40de970075d4d5a10c059e5371fff87/node_modules/eslint-config-react-app/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-react-app@7.0.0_e40de970075d4d5a10c059e5371fff87/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@8.9.0/node_modules/eslint/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@8.9.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-react-app@7.0.0_e40de970075d4d5a10c059e5371fff87/node_modules/eslint-config-react-app/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-react-app@7.0.0_e40de970075d4d5a10c059e5371fff87/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@8.9.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@8.9.0/node_modules/eslint/bin/eslint.js" "$@"
fi
