#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass@1.29.0/node_modules/sass/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass@1.29.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass-loader@10.0.5_sass@1.29.0+webpack@5.24.4/node_modules/sass-loader/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass-loader@10.0.5_sass@1.29.0+webpack@5.24.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass@1.29.0/node_modules/sass/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass@1.29.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass-loader@10.0.5_sass@1.29.0+webpack@5.24.4/node_modules/sass-loader/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/sass-loader@10.0.5_sass@1.29.0+webpack@5.24.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sass@1.29.0/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../sass@1.29.0/node_modules/sass/sass.js" "$@"
fi
