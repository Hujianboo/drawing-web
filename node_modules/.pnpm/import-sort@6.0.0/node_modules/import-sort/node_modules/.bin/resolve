#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/resolve@1.22.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/import-sort@6.0.0/node_modules/import-sort/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/import-sort@6.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/resolve@1.22.0/node_modules/resolve/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/resolve@1.22.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/import-sort@6.0.0/node_modules/import-sort/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/import-sort@6.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.0/node_modules/resolve/bin/resolve" "$@"
fi
