#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/browserslist@4.19.1/node_modules/browserslist/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/browserslist@4.19.1/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/postcss-colormin@5.2.5_postcss@8.4.6/node_modules/postcss-colormin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/postcss-colormin@5.2.5_postcss@8.4.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/browserslist@4.19.1/node_modules/browserslist/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/browserslist@4.19.1/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/postcss-colormin@5.2.5_postcss@8.4.6/node_modules/postcss-colormin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/postcss-colormin@5.2.5_postcss@8.4.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.19.1/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.19.1/node_modules/browserslist/cli.js" "$@"
fi
