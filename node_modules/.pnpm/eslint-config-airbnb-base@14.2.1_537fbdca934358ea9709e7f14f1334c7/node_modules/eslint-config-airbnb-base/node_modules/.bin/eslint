#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@7.13.0/node_modules/eslint/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@7.13.0/node_modules/eslint/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@7.13.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_537fbdca934358ea9709e7f14f1334c7/node_modules/eslint-config-airbnb-base/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_537fbdca934358ea9709e7f14f1334c7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@7.13.0/node_modules/eslint/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@7.13.0/node_modules/eslint/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint@7.13.0/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_537fbdca934358ea9709e7f14f1334c7/node_modules/eslint-config-airbnb-base/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/eslint-config-airbnb-base@14.2.1_537fbdca934358ea9709e7f14f1334c7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.13.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@7.13.0/node_modules/eslint/bin/eslint.js" "$@"
fi
