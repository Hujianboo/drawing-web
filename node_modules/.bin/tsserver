#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hujianbo/github/drawing-web/node_modules/.pnpm/typescript@4.0.8/node_modules/typescript/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/typescript@4.0.8/node_modules/typescript/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/typescript@4.0.8/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/typescript/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/typescript/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/typescript@4.0.8/node_modules/typescript/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/typescript@4.0.8/node_modules/typescript/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/typescript@4.0.8/node_modules:/Users/hujianbo/github/drawing-web/node_modules/.pnpm/node_modules:/Users/hujianbo/github/drawing-web/node_modules:/Users/hujianbo/github/node_modules:/Users/hujianbo/node_modules:/Users/node_modules:/node_modules:/Users/hujianbo/github/drawing-web/node_modules/typescript/bin/node_modules:/Users/hujianbo/github/drawing-web/node_modules/typescript/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
